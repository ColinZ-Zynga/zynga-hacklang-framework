{
  "name": "zynga/framework",
  "description": "Zynga Hacklang Framework - Collection of hack classes that enable us to utilize strict mode within zynga.",
  "type": "library",
  "keywords": [
    "hhvm",
    "hack",
    "framework"
  ],
  "require": {
    "hhvm": "^3.12",
    "zynga/phpunit": "^v1.1"
  },
  "autoload": {
    "psr-4": {
      "Zynga\\Framework\\AutoLoader\\V1\\": "src/Zynga/Framework/AutoLoader/V1/",
      "Zynga\\Framework\\Cache\\V2\\": "src/Zynga/Framework/Cache/V2/",
      "Zynga\\Framework\\CommandLine\\V1\\": "src/Zynga/Framework/CommandLine/V1/",
      "Zynga\\Framework\\Database\\V2\\": "src/Zynga/Framework/Database/V2/",
      "Zynga\\Framework\\Dynamic\\V1\\": "src/Zynga/Framework/Dynamic/V1/",
      "Zynga\\Framework\\ELO\\V1\\": "src/Zynga/Framework/ELO/V1/",
      "Zynga\\Framework\\Environment\\CodePath\\V1\\": "src/Zynga/Framework/Environment/CodePath/V1/",
      "Zynga\\Framework\\Environment\\DevelopmentMode\\V1\\": "src/Zynga/Framework/Environment/DevelopmentMode/V1/",
      "Zynga\\Framework\\Environment\\SuperGlobals\\V1\\": "src/Zynga/Framework/Environment/SuperGlobals/V1/",
      "Zynga\\Framework\\Exception\\V1\\": "src/Zynga/Framework/Exception/V1/",
      "Zynga\\Framework\\Factory\\V2\\": "src/Zynga/Framework/Factory/V2/",
      "Zynga\\Framework\\Performance\\V1\\": "src/Zynga/Framework/Performance/V1/",
      "Zynga\\Framework\\Testing\\BootStrap\\V2\\": "src/Zynga/Framework/Testing/BootStrap/V2/",
      "Zynga\\Framework\\Testing\\MockState\\V2\\": "src/Zynga/Framework/Testing/MockState/V2/",
      "Zynga\\Framework\\Testing\\TestCase\\V2\\": "src/Zynga/Framework/Testing/TestCase/V2/",
      "Zynga\\Framework\\Type\\V1\\": "src/Zynga/Framework/Type/V1/"
    }
  },
  "repositories": [
    { "packagist.org": false },
    { "type": "vcs", "url": "git@github.com:Zynga/zynga-hhvm-phpunit-hhi.git" },
    { "type": "vcs", "url": "git@github.com:Zynga/zynga-hhvm-phpunit.git" }
  ]
}
